{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "free": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "free"
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    },
    "pro": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "pro"
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    },
    "freeStore": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "free"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    },
    "proStore": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "pro"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "freeStore": {
      "ios": {
        "bundleIdentifier": "com.andrearizzo.balance",
